{
  "manifest_version": 2,
  "name": "Ramson Devtools",
  "description": "Devtools extension for debugging websocket connections with Ramson",
  "version": "0.1",
  "devtools_page": "src/js/devtools.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["wrapper.js"],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}